								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"pm.test(\"first match is SIT Career day\", function () {",
									"    var match = pm.response.json();",
									"    pm.expect(match.name).to.eql(\"SIT Career Day\");",
